package iproxys.PersistenceData;

/**
 *
 * @author root
 */

public class Layer4PDU extends Layer3PDU implements Comparable<Layer4PDU> {
    
    
    private int DstPort;
    private int SrctPort;
    private long  DataSize;
    
    /**
     * @return the DataSize
     */
    public long getDataSize() {
        return DataSize;
    }

    /**
     * @param DataSize the DataSize to set
     */
    public void setDataSize(long DataSize) {
        this.DataSize = DataSize;
    }

    /**
     * @return the SrctPort
     */
    public int getSrctPort() {
        return SrctPort;
    }

    /**
     * @param SrctPort the SrctPort to set
     */
    public void setSrctPort(int SrctPort) {
        this.SrctPort = SrctPort;
    }

    /**
     * @return the DstPort
     */
    public int getDstPort() {
        return DstPort;
    }

    /**
     * @param DstPort the DstPort to set
     */
    public void setDstPort(int DstPort) {
        this.DstPort = DstPort;
    }

      @Override
    public int compareTo(Layer4PDU t) {
        String[] split = getDstIP().split("\\.");
        String[] split1 = t.getDstIP().split("\\.");
        int ent1 = 0;
        int ent2 = 0;
        for(int i = 0; i < split.length ; i++){
            ent2 += Integer.parseInt(split1[i]);
            ent1 += Integer.parseInt(split[i]);
        }
        return (ent1 > ent2 )? -1 :(ent1 == ent2)?0 :-1 ;
        
    }
}
